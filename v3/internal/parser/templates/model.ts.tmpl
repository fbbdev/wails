{{- $pkg := .Package}}
{{- range .Imports}}
import * as {{.PackageName}} from "{{.Path}}/models";
{{- end}}{{if .Imports}}
{{end}}
{{- range $enumindex, $enumdef := .Enums}}
{{- if $enumdef.DocComment}}
/**
 * {{jsdoc $enumdef.DocComment}}
 */
{{- end}}
export enum {{$enumdef.Name}} {
    {{- range $constindex, $constdef := .Consts}}
    {{- if $constdef.DocComment}}{{if gt $constindex 0}}
{{end}}
    /**
     * {{jsdoc $constdef.DocComment}}
     */
    {{- end}}
    {{$constdef.Name}} = {{$constdef.Value}},{{end}}
}
{{end}}
{{- range $name, $def := .Models}}
{{- if $def.DocComment}}
/**
 * {{jsdoc $def.DocComment}}
 */
{{- end}}
export class {{$def.Name}} {
    {{- range $fieldindex, $fielddef := $def.Fields}}
    {{- if .DocComment}}{{if gt $fieldindex 0}}
{{end}}
    /**
     * {{jsdoc .DocComment}}
     */
    {{- end}}
    {{.JSName}}: {{.TSType $pkg}};{{if not .Exported}} // Warning: this is unexported in the Go struct.{{end}}{{end}}

    /** Creates a new {{$def.Name}} instance. */
    constructor(source: Partial<{{$def.Name}}> = {}) {
        const { {{- $def.DefaultValueList -}} } = source;
{{- range $def.Fields}}
        this.{{.JSName}} = {{.JSName}};{{end}}
    }

    /** Creates a new {{$def.Name}} instance from a string or object. */
    static createFrom(source: string | object = {}): {{$def.Name}} {
        let parsedSource = typeof source === 'string' ? JSON.parse(source) : source;
        return new {{$def.Name}}(parsedSource as Partial<{{$def.Name}}>);
    }
}
{{end -}}
