/**
 * This config governs the bindings generated by wails.  
 * It allows your project to have more strict configuration without causing errors in bindings.
 */
{
  "include": ["bindings"],
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "tsBuildInfoFile": "bindings/bindings.tsbuildinfo"
  }, 
}
